@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Página não existe!";
    var Erro = @TempData["ErrorCode"];
}

<h1 class="text-danger">Opa!</h1>
<h2 class="text-danger">Acho que a página que está procurando não existe, triste né? :(</h2>
<h2 class="warning">Código do Erro: @Erro</h2>
